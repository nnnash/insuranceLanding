.avatar-container {
	padding: 0 0 0 20px
}

.avatar-link {
	bottom: 40px;
	left: 30px;
	position: fixed
}

.avatar-link:link {
	text-decoration: none
}

.avatar-link:hover {
	text-decoration: none
}

.avatar-link:visited {
	text-decoration: none
}

.avatar-circle {
	width: 40px;
	height: 40px;
	border-radius: 50%;
	background-color: #cccccc;
	background-size: cover
}

.hidden {
	display: none !important;
	visibility: hidden !important
}

.ng-hide {
	display: block !important;
	position: absolute;
	top: -9999px;
	left: -9999px
}

.btn-2-lk {
	margin-top: 40px;
	width: 100%;
	font-size: 20px
}

.btn-2-lk-btn {
	width: 90%;
	display: block;
	margin: auto;
	padding: 5px
}

.home-bottom-wrap {
	display: flex;
	flex-direction: row;
	flex-wrap: nowrap;
	justify-content: space-between;
	align-content: center;
	align-items: flex-start
}

.home-bottom-white {
	background-color: white;
	border-radius: 2px;
	box-shadow: 0 8px 30px 0 rgba(0, 0, 0, 0.2);
	margin-bottom: 40px
}

.home-bottom-content {
	padding: 30px
}

.home-bottom-content h2 {
	color: #000000
}

.home-bottom-blue {
	width: 33%;
	max-width: 350px;
	border-radius: 2px;
	background-image: linear-gradient(233deg, #1eb6d6, #7b6dd8);
	box-shadow: 0 8px 30px 0 rgba(0, 0, 0, 0.2);
	color: #ffffff;
	margin-left: 40px;
	order: 2;
	flex: 0 1 auto;
	align-self: auto;
	min-height: 305px
}

.home-bottom-white-wrap {
	order: 1;
	flex: 0 1 auto;
	align-self: auto;
	width: 67%
}

.filter-fieldset {
	margin-bottom: 45px
}

.filter-fieldset__legend {
	font-size: 0.85rem;
	font-weight: bold;
	margin-bottom: 1rem;
	opacity: 0.85
}

.filter-fieldset .checkbox {
	margin-top: 0;
	font-size: 14px;
	margin-bottom: 9px;
	opacity: 0.9
}

.filter-cost__slider.ngrs-range-slider {
	margin: 10px 0
}

.filter-cost__slider.ngrs-range-slider .ngrs-value {
	display: none
}

.filter-cost__min, .filter-cost__max {
	display: inline-flex;
	width: 35%;
	border: 1px solid black;
	padding: 0 5px
}

.filter-cost__min {
	margin-right: 2%
}

.filter-product-data {
	margin-bottom: 20px
}

.filter-product-data .product-data__item {
	background: #e7e7e7;
	color: #222;
	border: 1px solid #e7e7e7;
	padding-top: 5px
}

.filter-product-data .product-data__item:hover {
	cursor: default

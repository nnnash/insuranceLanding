	flex-shrink: 0
}

.multicompany-result__content {
	position: relative;
	padding: 10px 19px;
	color: #000000;
	flex-shrink: 0
}

.multicompany-result__logo {
	max-width: 55%;
	max-height: 40px
}

.multicompany-result__list {
	margin: 0;
	padding: 0;
	list-style: none
}

.multicompany-result__title {
	font-size: 13px;
	line-height: 25px
}

.multicompany-result__title_disabled {
	color: #9B9B9B
}

.multicompany-result__rating {
	position: absolute;
	top: 15px;
	right: 15px;
	text-align: right;
	line-height: 1
}

.multicompany-result__rating div {
	text-align: right;
	font-family: "PT Sans Caption", sans-serif;
	font-size: 20px
}

.multicompany-result__conditions {
	list-style: none;
	padding-left: 0;
	margin-bottom: 40px
}

.multicompany-result__condition {
	font-size: 0.75rem;
	margin-bottom: 10px
}

.multicompany-result__condition-disabled {
	opacity: 0.5
}

.multicompany-result__franchise {
	position: absolute;
	display: inline-flex;
	bottom: 10px;
	padding: 0 5px;
	background: #F7E2B5;
	font-size: 0.875rem
}

.multicompany-result__footer {
	flex-shrink: 0;
	margin-top: auto;
	border-radius: 0 0 6px 6px;
	padding: 10px 19px 10px;
	color: #000;
	font-size: 13px;
	line-height: 1;
	white-space: nowrap
}

.multicompany-result__cost_rouble {
	font-size: 25px;
	position: relative;
	top: -6px
}

.multicompany-result:hover .multicompany-result__footer {
	background: #FFD300
}

.multicompany-result__cost {
	font-size: 34px;
	line-height: 34px;
	margin: 0 -3px
}

.container_check {
	display: flex;
	justify-content: space-between;
	flex-wrap: nowrap;
	align-items: stretch;
	align-content: stretch;
	height: 100%
}

.container_check .button {
	margin: auto
}

.container_check .insure-bottom-buttons {
	margin: 20px auto;
	display: flex
}

.check-panel {
	color: #4a4a4a
}

.check-panel__header {
	background: #ffffff;
	border-radius: 10px;
	padding: 15px
}

.check-panel-subsection__title {
	font-weight: bold;
	font-size: 16px
}

.check-panel-right {
	min-width: 25%;
	padding: 20px;
	color: #ffffff;
	height: 100%
}

.dashed-white-link {
	text-decoration: none !important;
	border-bottom: 1px dashed !important;
	color: #ffffff !important
}

.check-header__logo {
	max-width: 100%
}

.check-header__title {
	padding-top: 5px;
	font-weight: bold
}

.check-header__value {
	font-size: 0.875rem;
	font-weight: bold
}

.check-header__description {
	font-size: 0.7rem
}

.check-panel-subsection {
	border-radius: 10px;
	padding: 15px;
	margin-bottom: 10px
}

.check-panel-subsection-no-title {
	padding: 0 15px
}

.check-panel-section__title {
	padding: 15px 0 0 15px
}

.check-panel-field {
	width: 50%;
	float: left;
	margin-bottom: 20px
}

.check-panel-field__title {
	font-size: 12px
}

.check-panel-field__value {
	font-size: 14px
}

.contracts-container {
	margin-top: 20px;
	margin-left: 50%;
	width: 50%
}

.contract-card {
	margin-bottom: 10px;
	padding: 15px;
	border-radius: 15px;
	background: #ffffff;
	box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.5)
}

.contract-card__title {
	font-size: 32px
}

.contact-card__info {
	float: left;
	position: relative;
	width: 60%;
	min-height: 320px
}

.contract-card__sk {
	float: left;
	width: 40%
}

.contract-card__actions {
	position: absolute;
	bottom: 0
}

.contract-card__sk .multicompany-result {
	float: none
}

.form-field_marriageStatus {
	min-width: 300px
}

.combo-margin-bottom {
	margin-bottom: 15px
}

.load {
	width: 90%;
	margin: auto;
	margin-top: 30px;
	margin-bottom: 30px
}

.load p {
	font-size: 28px;
	color: #002D95;
	margin: auto;
	margin-top: 0px;
	margin-bottom: 10px;
	display: block;
	text-align: center
}

.load .range {
	width: 100%;
	height: 11px;
	background: #d6d6d6;
	border-radius: 10px;
	box-shadow: inset 0px 2px 5px 0px rgba(0, 0, 0, 0.21)
}

.load .range .line {
	height: 100%;
	width: 0%;
	background: #002D95;
	border-radius: 10px
}

.load .road {
	height: 34px;
	width: 0%
}

.load .road-container {
	margin-left: 120px
}

.load .road .car-run {
	float: right;
	width: 120px;
	height: 34px
}

.loading-animation {
	-webkit-animation: loading 5s infinite;
	-moz-animation: loading 5s infinite;
	animation: loading 5s infinite
}

@-webkit-keyframes loading {
	0% {
		width: 0%
	}
	100% {
		width: 100%
	}
}

@keyframes loading {
	0% {
		width: 0%
	}
	100% {
		width: 100%
	}
}

.back-arrow {
	padding: 3px 25px
}

.back-arrow a {
}

.back-arrow img {
	width: 36px;
	height: 23px
}

.product-data__item_driver {
	display: flex;
	margin: 0 0 5px -20px
}

.product-data__item_driver-nr {
	color: #000000;
	opacity: 0.3;
	margin-right: 7px
}

